---
layout: null
---

<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
	<channel>
		<title><![CDATA[{{ site.title }}]]></title>
		<description><![CDATA[{{ site.subtitle }}]]></description>
		<link href="{{ site.url }}/" />
		<atom:link href="{{ site.url }}/feed.xml" rel="self" type="application/rss+xml" />
		{% for doc in site.docs limit:30 %}
		<item>
      <title><![CDATA[{{ doc.title | cdata_escape }}]]></title>
      <link>{{ doc.url | prepend: site.baseurl | prepend: site.url }}</link>
      <pubDate>{{ doc.date | date: "%a, %d %b %Y %T %z" }}</pubDate>
      <category><![CDATA[{{ doc.category | cdata_escape }}]]></category>
      <description>
        <![CDATA[{{ doc.content | strip_html | truncatewords: 50 | expand_urls: site.url | cdata_escape }}]]>
      </description>
      <content:encoded>
        <![CDATA[{{ doc.content | expand_urls: site.url | cdata_escape }}]]>
      </content:encoded>
		</item>
		{% endfor %}
	</channel>
</rss>
